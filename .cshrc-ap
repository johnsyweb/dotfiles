setenv PATH /opt/python-2.5/bin:${PATH}
setenv PATH /sbin:${PATH}
setenv PATH /usr/ccs/bin:${PATH}
setenv PATH /usr/local/bin:${PATH}
setenv PATH /usr/sbin:${PATH}
setenv ARCH_BASE x86-solaris2.10

# Matt's useful stuff...
